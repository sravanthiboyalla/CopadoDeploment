        <externalId>false</externalId>
        <formula>AND(Age__c&gt;18,Weight__c&gt;50, TODAY()-Last_Donated_date__c&gt;90)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Eligible for Blood Donation</label>
