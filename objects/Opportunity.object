    <validationRules>
        <fullName>OppCloseLost</fullName>
        <active>true</active>
        <errorConditionFormula>AND((PRIORVALUE( StageName )==&apos;Closed Lost&apos;), ISCHANGED( StageName) )</errorConditionFormula>
        <errorMessage>you cannot change the status from closed lost to any other status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OppDescrition</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( StageName ,&apos;Closed Lost&apos;),ISBLANK( Description),ISNEW())</errorConditionFormula>
        <errorMessage>when opportunity is closed lost descriptin is mandatory field.</errorMessage>
    </validationRules>
